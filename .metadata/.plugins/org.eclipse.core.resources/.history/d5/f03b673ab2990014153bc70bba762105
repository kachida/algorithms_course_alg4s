package stacks;

public class Stack {
	
	private class Node
	{
		 String item;
		Node next;
		
	}
	
	public String pop()
	{
		
		item=first.item;
		first=first.next;
		return item;
		
	}
	
	public void push(String item)
	{
		Node oldfirst=first;
		first = new Node();
		first.item="kanna";
		first.next=oldfirst;
	}

}
